<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>com</groupId>
    <artifactId>ReaderBench</artifactId>
    <version>1.0</version>
    <packaging>jar</packaging>
    <build>
        <pluginManagement>
            <plugins>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-install-plugin</artifactId>
                    <version>2.5.2</version>
                    <executions>
                        <execution>
                            <id>1</id>
                            <phase>clean</phase>
                            <goals>
                                <goal>install-file</goal>
                            </goals>
                            <configuration>
                                <groupId>org</groupId>
                                <artifactId>knowceans-tools</artifactId>
                                <version>1</version>
                                <packaging>jar</packaging>
                                <file>${project.basedir}/resources/lib/knowceans-tools.jar</file>
                            </configuration>
                        </execution>
                        <execution>
                            <id>2</id>
                            <phase>clean</phase>
                            <goals>
                                <goal>install-file</goal>
                            </goals>
                            <configuration>
                                <groupId>org</groupId>
                                <artifactId>gephi-toolkit</artifactId>
                                <version>0.8.7</version>
                                <packaging>jar</packaging>
                                <file>${project.basedir}/resources/lib/gephi-toolkit-0.8.7.jar</file>
                            </configuration>
                        </execution>
                        <execution>
                            <id>3</id>
                            <phase>clean</phase>
                            <goals>
                                <goal>install-file</goal>
                            </goals>
                            <configuration>
                                <groupId>edu.stanford.nlp</groupId>
                                <artifactId>stanford-corenlp-caseless</artifactId>
                                <version>3.5.2</version>
                                <packaging>jar</packaging>
                                <file>${project.basedir}/resources/lib/stanford-corenlp-caseless-2015-04-20-models.jar</file>
                            </configuration>
                        </execution>
                        
                    </executions>
                </plugin>
            </plugins>
        </pluginManagement>
    </build>
